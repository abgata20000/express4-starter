config:
  fname: "defalut.yaml"
  servicename: "SampleApp"
  version: "0.0.1"
  servername: http://sample.com
  serverNum: 1
  port: 3000
  basicAuth:
    user: user
    password: password
  mongo:
    name: app_dbname
    host: localhost
    port: 27017
    user:
    password:
  session:
    type: redis
    secret: secret
    mongo:
      dbname: session_dbname
      host: localhost
      port: 27017
      user:
      password:
    redis:
      host: localhost
      port: 6379
      prefix: mysession
      ttl: 604800
      disable_ttl: true
  aws:
    accessKeyId:
    secretAccessKey:
    bucket:
    region: ap-northeast-1
    s3HostName: s3-ap-northeast-1.amazonaws.com

  redis:
    host: localhost
    port: 6379
